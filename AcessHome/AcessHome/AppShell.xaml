<?xml version="1.0" encoding="utf-8" ?>
<Shell
    x:Class="AcessHome.AppShell"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AcessHome"
    xmlns:views="clr-namespace:AcessHome.Views"
    xmlns:vm="clr-namespace:AcessHome.ViewModels">

    <ShellContent
        Title="Inicio"
        Icon="home.png"
        ContentTemplate="{DataTemplate views:AdminView}" />

    <ShellContent
        Title="Visitas"
        ContentTemplate="{DataTemplate views:VisitasView}"
        Icon="visitas.png" />

    <ShellContent
        Title="Usuarios"
        ContentTemplate="{DataTemplate views:UsersView}"
        Icon="usuarios.png" />


    <ShellContent
        Title="Solicitudes"
        ContentTemplate="{DataTemplate views:UsersRequestView}"
        Icon="request.png" />





